<p id="notice"><%= notice %></p>


<table>
  <thead>
    <tr>
      <th>Skill</th>
      <th>Hours studied</th>
      <th>Code</th>
      <th colspan="3"></th>
    </tr>
  </thead>

<div class="container-body">
    <% @skillsets.each do |skillset| %>
      <tr>
        <td><%= skillset.skill %></td>
        <td><%= skillset.hours_studied %></td>
        <td><%= skillset.code %></td>
        <td><%= link_to 'Show', skillset %></td>
        <td><%= link_to 'Edit', edit_skillset_path(skillset) %></td>
        <td><%= link_to 'Destroy', skillset, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>

         </div>


</table>

<%= link_to 'Add a new skill', new_skillset_path %>
